{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "commonjs",
      "lib": [
          "es6",
          "es2015",
          "es7"
      ],
    "moduleResolution": "node",
    "paths": {
      "@data": ["src/lib/data"],
      "@events": ["src/lib/events"]
